    "eslint": "^8.47.0",
    "eslint-config-cvs": "workspace:*",
    "eslint-config-next": "^13.4.13",
    "jsdom": "^22.1.0",
    "postcss": "^8.4.27",
    "prettier": "^3.0.1",
    "prettier-plugin-tailwindcss": "^0.5.2",
    "tailwind-config": "workspace:*",
    "tailwindcss": "^3.3.3",
